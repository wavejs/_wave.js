{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "excludeTags": [],
    "parser": "gfm",
    "hardwrap": true
  },
  "templates": {
    "logoFile": "",
    "cleverLinks": true,
    "monospaceLinks": false,
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "VV",
    "path": "ink-docstrap",
    "theme": "paper",
    "copyright": "VV Â© 2012-2015 The contributors to the JSDoc3 and DocStrap projects.",
    "navType": "inline",
    "linenums": true,
    "collapseSymbols": false,
    "methodHeadingReturns": false,
    "dateFormat": "MMMM YYYY, h:mm:ss a"
  }
}
